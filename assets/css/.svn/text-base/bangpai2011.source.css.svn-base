	float:left;
	width:20px;
	height:20px;
	margin:0 3px 0 0;
	overflow:hidden;
	text-align:center;
	white-space:nowrap;
	vertical-align:middle;
	line-height:20px;
}
.page-mini a, .page-mini .page-cur, .page-mini .page-prev, .page-mini .page-cur, .page-mini .page-start, .page-mini .page-end, .page-mini .page-break {
	height:14px;
	width:auto;
	padding:0 2px;
	line-height:16px;
}
.pagination a {
	border:1px solid #ccc;
	background-color:#fff;
}
.page-mini a {
	border:none;
	background-color:transparent;
}
.pagination .page-100 {
	width:26px;
}
.page-mini .page-100 {
	width:auto;
}
