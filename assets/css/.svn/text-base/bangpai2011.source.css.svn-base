/* Modules: =修正部分
 * 修正门户帮派950宽度、页尾
-------------------------------------------------------------- */
#page {
	width:990px;
}
#content {
	margin-top:10px;
}
.grid-c2f .main-wrap {
	margin-right: 210px;
}
.grid-c2f .col-sub {
	margin-left: -190px;
	width: 190px;
	background-color:#FAFAFA;
}
#footer {
	width:990px;
	border-top: 3px solid #7F7F7F;
	clear: both;
	margin: 10px auto;
	padding-top: 10px;
	text-align: center;
}
#footer .foot-nav a, #footer .ali-group a {
	color:#5C5C5C;
}
/* css sprites */
.search-auto, .search-form .input, .search-tab li.selected, .search-tab li.selected .l, .search-tab li.selected .r, .search-form .l, .search-form form button, .search-form .r {
	background: url("http://img01.taobaocdn.com/tps/i1/T1.udLXn4aXXXXXXXX.png") repeat-x scroll 0 0 transparent;
}
.marrow .bd .item .button, .invitation .hd, .faction .hd, .tao-friend .hd, .gamepage .button {
	background:url("http://img03.taobaocdn.com/tps/i3/T1rR04XfJbXXXXXXXX-100-300.png") no-repeat scroll -999em -999em transparent;
}
/* Modules: =公用部分
-------------------------------------------------------------- */
.result-info .result-tip {
	background: none repeat scroll 0 0 #F7F7F7;
	border-top:#F27602 solid 3px;
	margin-bottom: 10px;
	padding: 5px 3px;
}
.result-info .result-tip em {
	color: #FF5500;
	font-weight: 700;
}
/* 顶部tab  */
.bangpai-tabs {
	width: 988px;
	height:30px;
	border:#EBEBEB solid 1px;
	border-bottom:#BFCBCB solid 1px;
	background-color:#F7F7F7;
	margin-bottom:15px;
}
.bangpai-tabs .ks-switchable-nav {
	width:600px;
	height:30px;
	position:relative;
}
.bangpai-tabs .ks-switchable-nav li {
	position:absolute;
	width: 100px;
	height: 30px;
	line-height:30px;
	font-size:14px;
	text-align: center;
}
.bangpai-tabs .ks-switchable-nav li a {
	text-decoration:none;
	color:#057BD2;
	text-align: center;
	cursor:pointer;
	}
.bangpai-tabs .ks-switchable-nav li.all { 
    top:0px;
    left:0px;
	background-position:0 -233px;
}
.bangpai-tabs .ks-switchable-nav li.tiezi { 
    top:0px;
    left:100px;
	background-position:0 -233px;
}
.bangpai-tabs .ks-switchable-nav li.bangpai { 
    top:0px;
    left:200px;
	background-position:0 -233px;
}
.bangpai-tabs .ks-switchable-nav li.game { 
    top:0px;
    left:300px;
	background-position:0 -233px;
}
.bangpai-tabs .ks-switchable-nav li.taofriend { 
    top:0px;
    left:400px;
	background-position:0 -233px;
}
.bangpai-tabs .ks-switchable-nav li.bgnone { background:none;}
.bangpai-tabs .ks-switchable-nav li.ks-active  {
	top:0px;
	height:31px;
	background-position:0 -198px;
}
.bangpai-tabs .ks-switchable-nav li.ks-active a {
	cursor: default;
	color:#CC0000;
	font-size:14px;
	font-weight:bold;
}
/* 底部搜索 */
.bottom-wrapper {
	width: 990px;
	clear:both;
}
.search-bottom {
	padding:20px 0;
	background:#F5F5F5;
	margin:10px 0 0;
	width:auto;
	height:auto;
}
.search-form legend {
	display: none;
}
.search-bottom form {
	width:572px;
	margin:0 auto;
}
.search-form form .input {
	background-color: #FFFFFF;
	background-position: 0 -68px;
	float: left;
	height: 32px;
	position: relative;
	width: 463px;
}
.search-bottom form .input {
	width:441px;
	overflow:hidden;
}
.search-form form .input input {
	background: none repeat scroll 0 0 transparent;
	border: medium none;
	font: 14px/18px verdana, tahoma, arial, 宋体, sans-serif;
	height: 18px;
	left: 0;
	padding: 7px 6px;
	position: absolute;
	top: 0;
	width: 290px;
}
.search-bottom form .input input {
	width:431px;
	_width:427px;
}
.search-bottom .show-cate, .search-bottom .search-tab {
	display:none;
}
.search-form form .input .l {
	background-position: -136px -36px;
	left: 0;
}
.search-form form .input .l, .search-form form .input .r {
	height: 32px;
	position: absolute;
	top: 0;
	width: 3px;
}
.search-form form .input .r {
	background-position: -139px -36px;
	right: 0;
	z-index: 1;
}
.search-form form .input .l, .search-form form .input .r {
	height: 32px;
	position: absolute;
	top: 0;
	width: 3px;
}
.search-form form button {
	background-position: 0 -36px;
	border: medium none;
	float: left;
	height: 32px;
	text-indent: -9999px;
	width: 131px;
	cursor:pointer;
}
/* Modules: =帖子搜索
-------------------------------------------------------------- */
.marrow {
	height:310px;
	background-color:#FAFAFA;
	border:#EEEEEE solid 1px;
	position:relative;
	z-index:1;
	margin-bottom:40px;
}
.marrow .hd h3 {
	color:#808080;
	margin:15px 0px;
	font-weight:normal;
	text-indent:20px;
}
.marrow .bd ul {
	margin:0px 0px 0px 22px;
}
.marrow .bd .item {
	display: inline;
	float: left;
	width: 130px;
	overflow:hidden;
	text-align:left;
	margin-right:21px;
	line-height:20px;
	color:#808080;
}
.marrow .bd .item .pic {
	/* 非IE的主流浏览器识别的垂直居中的方法 */
	display: table-cell;
	vertical-align:middle;
	/* 设置水平居中 */
	text-align: center;
	/* 针对IE的Hack */
	*display: block;
 *font-size: 105px; /*约为高度的0.873，200*0.873 约为175*/
 *font-family:Arial;
	width: 120px;
	height: 120px;
	border: 0 none;
	background-color: #FFFFFF;
	overflow: hidden;
}
.marrow .bd .item .pic a {
	display:inline;
}
.marrow .bd .item .pic img {
	vertical-align:middle;
	border:none;
}
.marrow .bd .item .title {
	color:#057BD2;
	margin-top:5px;
	height:20px;
	width:100%;
	overflow:hidden;
}
.marrow .bd .item .time span {
	color:#3D3D3D;
}
.marrow .bd .item .price span {
	color:#3D3D3D;
}
.marrow .bd .item .tprice span {
	color:#CC0000;
	font-weight:bold;
}
.marrow .bd .item .source a:hover {
	text-decoration:none;
}
.marrow .bd .item .button {
	width:63px;
	height:22px;
	line-height:22px;
	background-position:-0px -0px;
	text-align:center;
	margin-top:5px;
}
.marrow .bd .item .button a {
	color:#4D4D4D;
}
.marrow .bd .item .button a:hover {
	text-decoration:none;
}
.marrow .more {
	width:85px;
	position:absolute;
	top:15px;
	right:10px;
	background:none;
	text-align:right;
}
.marrow .more a {
	color:#057BD2;
}
.marrow .more a span {
	color:#019308;
}
.marrow .more a:hover {
	text-decoration:none;
	color:#ff6600;
}
