/* pagination */
.pagination {
	height:22px;
	font-size:12px;
	font-family:Tahoma;
	clear:both;
}
.page-top:after, .page-bottom:after {
	content:"";
	display:block;
	height:0;
	clear:both;
	visibility:hidden;
}
.page-top, .page-bottom {
	zoom:1;
}
#pagination_div .page-top {
	float:right;
	margin-right:60px;
}
.page-mini {
	padding-top:3px;
}
