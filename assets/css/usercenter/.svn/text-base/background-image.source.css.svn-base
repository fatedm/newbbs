/* background-image */

.background-image .size-s {
    background-position: -1px -50px;
}

.background-image .size-l {
     background-position: -98px -50px;
}

.background-image .size-xl {
     background-position: -1px -100px;
}

.background-image .size-m.h {
     background-position: -42px 0;
}

#popup-msg .h {
     background-position: -42px 0;
}


.background-image .size-l.h {
     background-position: -98px 0;
}

.background-image .size-s:hover {
     background-position: -1px -75px;
}

.background-image .size-l:hover {
     background-position: -98px -75px;
}

.background-image .size-xl:hover {
     background-position: -1px -125px;
}

.background-image .size-m.h:hover {
    background-position: -42px -25px;
}

.background-image .size-l.h:hover {
    background-position: -98px -25px;
}
/* fixed-size */
.fixed-size .size-s,
.fixed-size .size-m,
.fixed-size .size-l,
.fixed-size .size-xl,
button.size-s,
button.size-m,
button.size-l,
button.size-xl {
	 border: 0px;
	 height:24px;
}

.fixed-size .size-s {
     width: 40px;
}

.fixed-size .size-m {
    width: 55px;
	color:#fff!important;
}

.fixed-size .size-l {
    width: 80px;
}

.h{
    color:#fff!important;
}

.fixed-size .size-xl {
    width: 115px;
}

button.size-s {
    width: 40px;
}

button.size-m {
    width: 55px;
}

button.size-l {
    width: 80px;
}

button.size-xl {
    width: 115px;
}