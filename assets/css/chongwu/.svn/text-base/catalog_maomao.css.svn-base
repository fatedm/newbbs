}

/* shows */
.grid-wrap .shows .hd h2 {
	width: 83px;
	height: 22px;
	background-position: 0px -260px;
}

.grid-wrap .shows ul {
	list-style: none;
	display: block;
}

.grid-wrap .shows li {
	position: relative;
	width: 270px;
	height: auto;
	border: solid 1px #cacaca;
	margin-bottom: 10px;
	text-align: center;
}

.grid-wrap .shows li img {
	width: 270px;
	height: auto;
}

.grid-wrap .shows li p {
	display: block;
	position: absolute;
	bottom: 0px;
	left: 0px;
	width: 260px;
	height: 25px;
	padding: 0 5px;
	background: #fff;
	opacity: 0.8;
	filter: alpha(opacity=80);
	color: #333;
	line-height: 25px;
}

.grid-wrap .shows li a {
	display: block;
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 2;
	width: 270px;
	height: 100%;
	background: url("about:blank");
}

.grid-wrap .grid-main .list,
.grid-wrap .grid-sub .shows {
	margin-bottom: 40px;
}

