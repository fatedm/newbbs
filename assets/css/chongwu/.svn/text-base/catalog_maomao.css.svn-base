#J_slide_play {
		position: relative;
		width: 240px;
        height: 300px;
        /* border: 1px solid #B6D1E6; */
        overflow: hidden;
    }

    #J_slide_play .ks-switchable-nav {
        position: absolute;
        bottom: 5px;
        right: 5px;
        z-index: 99;
    }

    #J_slide_play .ks-switchable-nav li {
        float: left;
        width: 16px;
        height: 16px;
        line-height: 16px;
        margin-left: 2px;
        background-color: #fff;
        /*border: 1px solid #F47500;*/
        color: #000;
        text-align: center;
        cursor: pointer;
        list-style: none;

    }

    #J_slide_play .ks-switchable-nav li.ks-active {
        width: 16px;
        height: 16px;
        line-height: 16px;
        margin-top: 0;
        color: #FFF;
        background-color: #B56DB5;
    }

    #J_slide_play .ks-switchable-content {
        margin: 0;
        padding: 0;
    }
    #J_slide_play .ks-switchable-content li {
        height: 300px;
        width: 240px;
        overflow: hidden;
        margin: 0;
    }
    #J_slide_play .ks-switchable-content li a{
		position:relative;
	}
    #J_slide_play .ks-switchable-content li a:hover{
        text-decoration:none;
    }
	#J_slide_play .ks-switchable-content li a span{
		position:absolute;
		bottom:10px;
		display:inline-block;
		color:#fff;
		left:10px;
		font-weight:bold;
		cursor:pointer;
	}
    #J_slide_play .ks-switchable-nav li,
    #J_slide_play .ks-switchable-trigger-content {
        position: relative;
		border-radius:8px;
    }

    #J_slide_play .ks-switchable-trigger-mask {
        position: absolute;
        right: 0;
        width: 16px;
        height: 16px;
        background-color: none;
        visibility: hidden;
		border-radius:8px;
    }

    #J_slide_play .ks-active .ks-switchable-trigger-mask {
        visibility: visible
    }  }