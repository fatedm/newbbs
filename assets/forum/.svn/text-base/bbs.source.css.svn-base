#J-getAbsolute-forum-map {
	position:relative;
	z-index:2;
	margin:0 auto;
	width:950px;
	height:0;
	font-size:0;
	line-height:0;
}

#absolute-forum {
	border:solid 3px #c0c0c0;
	position:absolute;
	right:0;
	top:80px;
	font-size:12px;
	line-height:normal;
}

#absolute-forum b {
	position:absolute;
	right:12px;
	top:10px;
	font-weight:normal;
	cursor:pointer;
}

#J-absolute-forum {
	width:658px;
	background:#fff;
	border:solid 1px #9f9f9f;
	padding:21px 20px 0 24px;
}

#J-absolute-forum div {
	border-bottom:solid 1px #eaeaea;
	padding:8px 0 6px 0;
	position:relative;
	width:100%;
}

#J-absolute-forum div:after {
	content:".";
	display:block;
	height:0;
	clear:both;
	visibility:hidden;
}

#J-absolute-forum h3 {
	position:absolute;
	left:0;
	top:7px;
	width:70px;
	font-size:14px;
	color:#4d4d4d;
	font-weight:normal;
}

