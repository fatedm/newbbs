/*头像编辑，帮徽编辑*/
.avatar-edit{
	position:relative;
	width:610px;
	margin:0 auto;
	background:url(http://a.tbcdn.cn/app/matrix/img/avatar_upload.png) no-repeat 415px 210px;
}
.avatar-edit .original{
	width:400px;
	height:400px;
	border:1px dashed #E6E6E6;
	background-color:#F8F8F8;
	overflow:hidden;
}
