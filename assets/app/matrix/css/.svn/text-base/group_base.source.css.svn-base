h1, h2, h3, h4, h5, h6, strong {
	font-weight:bold;
}.example{
	width:500px;
	padding:30px;
	margin:0 auto;
}
/*无图片时alt信息溢出隐藏,for ff*/
.avatar img{
	overflow:hidden;
	display:block;
	margin:0 auto;
	*display:inline;/*ie忽略*/
}
.card,.nameplate,.nameplate .master,.nameplate .assist,.avatar span,.card .title,.group .pic,p.full{
	background:url(http://a.tbcdn.cn/app/matrix/img/avatar_bg.png) no-repeat 0 9999px;
}
.pic{
	background-color:#fff;
    text-align:center;
    display: table-cell;
    vertical-align:middle;
    *display: block;
	*overflow:hidden;
}
.pic img{/*for ie*/
	vertical-align:middle;
}
.s30{
	width:30px;
	height:30px;
	line-height:30px;
}
.s30 img{
	max-height:30px;
	max-width:30px;
	height:auto!important;
	width:auto!important;
	*height:30px;
	*width:30px;
	vertical-align:top;
}
.s40{
	width:40px;
	height:40px;
	line-height:40px;
}
.s40 img{
	max-height:40px;
	max-width:40px;
	height:auto!important;
	width:auto!important;
	*height:40px;
	*width:40px;
	vertical-align:top;
}
.s60{
	width:60px;
	height:60px;
	line-height:60px;
    *font-size: 45px;
}
.s60 img{
	max-height:60px;
	max-width:60px;
	height:auto!important;
	width:auto!important;
	*height:60px;
	*width:60px;
}
.avatar.group .s60 img{
	max-height:40px;
	max-width:40px;
	height:auto!important;
	width:auto!important;
	*height:40px;
	*width:40px;
}
.s80{
	width:80px;
	height:80px;
	line-height:80px;
    *font-size:60px;
}
.s120{
	width:120px;
	height:120px;
	line-height:120px;
	*font-size:100px;
}
.s120 img{
	max-height:120px;
	max-width:120px;
	height:auto!important;
	width:auto!important;
	*height:120px;
	*width:120px;
	*vertical-align:baseline;
}
.s160{
	width:160px;
	height:160px;
}
.s180{
	width:180px;
	height:180px;
	line-height:180px;
	*font-size: 133px;
	vertical-align:middle;
}
.avatar{
	position:relative;
	display:-moz-inline-stack;
	display:inline-block;
	*display:inline;
	*zoom:1;
}
.avatar .pic{
	border:1px solid #BBCCDD;
	padding:1px;
}
/*for ie bug
.avatar .pic.s40{
	*padding:1px 2px 2px 1px;
}*/
.avatar span{
	display:none;
	position:absolute;
	right:-5px;
	top:0;
	height:31px;
	width:7px;
	font:0/0 arial;
	background-position:-564px -121px;
	z-index:100;
}

.card{
	width:50px;
	height:80px;
	background-position:-360px -80px;
	padding:0 15px;/*15px+50px+15px*/
	text-align:center;
	position: relative;
	overflow:hidden;
}
.card .avatar{
	margin-top: 17px;
}
.card:hover{
	background-position:-440px -80px;
}
.card .master,.card .assist{
	width:42px;
	height:13px;
	float:left;
	margin:0 0 -13px 4px;/*(50-42)/2*/
	*line-height:15px!important;
	*display:inline;
	*zoom:1;
	line-height:13px;
	color:#fff;
}
.card .master{
	background-position:-521px -121px;
}
.card .assist{
	background-position:-521px -136px;
}
.nameplate{
	width:190px;
	height:57px;
	background-position:-580px 0;
}
.nameplate:hover{
	background-position:-580px -57px;
}
.nameplate .master,.nameplate .assist{
	float:left;
	width:12px;
	padding:0 7px 0 2px;
	line-height:13px;
	height:45px;
	margin:6px 0 0 0;
	color:#fff;
}
.nameplate .master{
	background-position:-774px -62px;
	padding-top:9px;
	height:36px;/*45-9*/
}
.nameplate .assist{
	background-position:-774px -7px;
	padding-top:3px;
	height:42px;/*45-3*/
}
.nameplate .info{
	float:left;
	width:163px;
	overflow:hidden;
	padding:6px 0 0 6px;
}
.avatar.group{
	width:auto;
	height:auto;
	zoom:1;
}
.avatar.group .pic{
	border:none;
	padding:0;
}
.avatar.group .s180{
	background-position:0 0;
}
.avatar.group .s180:hover{
	background-position:-180px 0;
}
.avatar.group .s80{
	background-position:-360px 0;
}
.avatar.group .s80:hover{
	background-position:-440px 0;
}
.avatar.group .s60{
	background-position:-520px 0;
}
.avatar.group .s60:hover{
	background-position:-520px -60px;
}

/*小彗星：特小(xs-30),小(s-40),中(m-60),大(l-80)*/
