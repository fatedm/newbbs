.box .hd,.box .hd h3,
.box .rc-tp, .box .rc-tp span,
.box .rc-bt, .box .rc-bt span ,.box.stone .bd,.info .bd .avatar{
    background: url(http://a.tbcdn.cn/app/matrix/img/box_bg.png) no-repeat;
}
.box .rc-tp, .box .rc-bt {
    position: relative; /* raise z-index, hover .hd */
    display: block;
    height: 5px;
	overflow: hidden;
	z-index:1000;
}
.box .rc-tp span, .box .rc-bt span {
    float: right;
    width: 5px;
    height: 5px;
}
.box .rc-tp {
    margin-bottom: -5px;
    background-position: -4px 0;
}
.box .rc-bt {
    margin-top: -5px;
    background-position: -4px -4px;
}
.box .rc-bt span {
    background-position: 0 -4px;
}

.box {
    position: relative; /* for .act list */
    margin-bottom: 10px; /* default gutter */
}
.box .hd {
	background-position:0 -9px;
    height: 29px;
    border: 1px solid #d6d6d6;
    border-bottom: none;
	margin-bottom:-1px;
	position:relative;
	background-color:#fff;
	z-index:900;
}
.box .hd h3{
    /*font-size: 12px;*/
	font-size:14px;
    line-height: 29px; /* 11px + 9px + 9px */
	height:29px;
	overflow:hidden;
    color: #404040;
	margin: 0 10px;
    background-position: 0 -11px;
    background-repeat: repeat-x;
}
.box .hd .avatar{
    background-position: 0 bottom;
    background-repeat: repeat-x;
}
.box-h14 .hd h3,.manage .hd h3{
	font-size:14px;
}
.box-hot .hd h3,.box h3.hot,.hot a,.hot a:visited{
	color:#f60;
}
.box .bd {
    padding: 10px;
    border: 1px solid #d6d6d6;
	bor/der-top:none;
	height: auto!important;
	*height:10px;
	min-height:10px;
	*position:relative;
}

.box .ft .act {
    position: absolute;
    top: 1px;
    right: 1px;
    line-height: 29px;
    padding-right: 10px;
	z-index:1000;
}
.box .ft .act li {
    float: left;
    padding-left: 10px;
}
/*灰度渐变容器robot*/
.box.manage .hd,.box.robot .hd{
	background-position:0 -45px;
