	height:160px;
	width:160px;
	border:1px solid #e4e4e4;
	position:relative;
	overflow:hidden;
	display:block;
}
.avatar-edit .preview .pic img{
	left:0;
	top:0;
	position:absolute;
}
.avatar-edit .action{
	margin:10px 208px 10px 0;
	text-align:right;
}
